<zapaddon>
	<name>HUD - Heads Up Display</name>
	<version>@version@</version>
	<status>@status@</status>
	<description>Display information from ZAP in browser.</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>@changes@</changes>
	<dependencies>
		<addons>
			<addon>
				<id>websocket</id>
			</addon>
		</addons>
	</dependencies>
	<extensions>
		<extension>org.zaproxy.zap.extension.hud.ExtensionHUD</extension>
		<extension v="1">
			<classname>org.zaproxy.zap.extension.hud.launch.ExtensionHUDlaunch</classname>
			<classnames>
				<allowed>org.zaproxy.zap.extension.hud.launch</allowed>
			</classnames>
			<dependencies>
				<addons>
					<addon>
						<id>selenium</id>
						<semver>2.*</semver>
					</addon>
				</addons>
			</dependencies>
		</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files>
@files@
	</files>
	<not-before-version>@zapVersion@</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>
