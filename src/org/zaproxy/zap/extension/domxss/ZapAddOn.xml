<zapaddon>
	<name>DOM XSS Active scanner rule</name>
	<version>3</version>
	<status>alpha</status>
	<description>DOM XSS Active scanner rule</description>
	<author>ZAP Dev Team</author>
	<url/>
	<changes>
	<![CDATA[
	Skip the scanner if not able to start Firefox.<br>
	]]>
    </changes>
	<dependencies>
		<addons>
			<addon>
				<id>selenium</id>
				<semver>1.*</semver>
			</addon>
		</addons>
	</dependencies>
	<extensions>
		<extension>org.zaproxy.zap.extension.domxss.ExtensionDomXSS</extension>
	</extensions>
	<ascanrules>
		<ascanrule>org.zaproxy.zap.extension.domxss.TestDomXSS</ascanrule>
	</ascanrules>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.4.1</not-before-version>
	<not-from-version/>
</zapaddon>
