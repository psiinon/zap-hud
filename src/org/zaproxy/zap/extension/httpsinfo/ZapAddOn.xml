<zapaddon>
	<name>HttpsInfo</name>
	<version>8</version>
	<status>alpha</status>
	<description>Displays HTTPS status information.</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>
	<![CDATA[
	Warn the user if results may be inaccurate due to ZAP being configured to use an outbound proxy.<br>
	]]>
	</changes>
	<extensions>
	    <extension>org.zaproxy.zap.extension.httpsinfo.ExtensionHttpsInfo</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.4.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>

